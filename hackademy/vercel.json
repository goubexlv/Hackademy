{
    "version": 2,
    "builds": [
      {
        "src": "hackademy/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3" }
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "static/$1" },
      { "src": "/(.*)", "dest": "hackademy/wsgi.py" }
    ]
  }
  